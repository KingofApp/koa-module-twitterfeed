{
  "name": "Twitter Feed",
  "identifier": "twitterfeed",
  "description": {
    "es-ES": "Activando esta opción, tus seguidores podrán leer todo cuanto publiques en Twitter. Para configurarlo, entra la url de tu perfil. P.e.  https://twitter.com/theKingofApp",
    "en-US": "Activando esta opción, tus seguidores podrán leer todo cuanto publiques en Twitter. Para configurarlo, entra la url de tu perfil. P.e.  https://twitter.com/theKingofApp"
  },
  "documentation": {
    "es-ES": "modules/twitterfeed/documentation/es_ES.md",
    "en-US": "modules/twitterfeed/documentation/en_US.md"
  },
  "descriptionShort": {
    "es-ES": "Activando este módulo tus seguidores podrán leer todo cuanto publiques en Twitter.",
    "en-US": "Activando este módulo tus seguidores podrán leer todo cuanto publiques en Twitter."
  },
  "icon": "twitter",
  "type": "A",
  "version": "1.0.4",
  "author": "King of App",
  "category": [
    "social"
  ],
  "price": 0,
  "subscription": false,
  "canContain": false,
  "platforms": [
    "android",
    "ios",
    "windows"
  ],
  "view": "modules/twitterfeed/index.html",
  "files": [
    "modules/twitterfeed/controller.js",
    "modules/twitterfeed/directive.js",
    "modules/twitterfeed/style.css",
    "modules/twitterfeed/style2.css"
  ],
  "scope": {
    "widgetid": "",
    "limite": ""
  },
  "config": [{
    "templateOptions": {
      "label": "widgetid",
      "description": "id del widget",
      "pattern": "^[a-zA-Z0-9]+$",
      "required": true
    },
    "type": "input",
    "key": "widgetid"
  }, {
    "type": "input",
    "key": "limite",
    "templateOptions": {
      "label": "limite",
      "pattern": "^[0-9]+$",
      "required": true,
      "maxlength": 2,
      "description": "limite tweets"
    }
  }],
  "showOn": {
    "menu": true,
    "market": true,
    "dragDrop": true
  },
  "images": {
    "list": "modules/twitterfeed/images/twitter_list.png",
    "screenshots": [
      "modules/twitterfeed/images/twitter_screenshot.png"
    ],
    "popover": "modules/twitterfeed/images/twitter_popover.png",
    "banner": "modules/twitterfeed/images/twitter_popover.png",
    "logo": "modules/twitterfeed/images/twitter_icon.png"
  }
}
